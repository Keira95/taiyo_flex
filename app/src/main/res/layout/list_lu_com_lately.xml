<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/lu_order_list"
    android:background="@drawable/textborder5">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:orientation="horizontal"
        android:background="@drawable/grid_header_border"
        >

        <FrameLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#12000000"
            app:layout_constraintEnd_toEndOf="@+id/guideline_1"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            >

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />

            <TextView
                android:id="@+id/t2_work_order_no"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:textSize ="9dp"
                android:autoSizeStepGranularity="1px"


                android:gravity="center"
                 />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:id="@+id/t2_week_actual_qty"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize ="14sp"
                android:autoSizeStepGranularity="1px"
                android:layout_gravity="center"
                android:gravity="center"
                />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:id="@+id/t2_equipment_name"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize ="12sp"
                android:autoSizeStepGranularity="1px"
                android:layout_gravity="center"
                android:gravity="center"
                />

        </LinearLayout>

        <FrameLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#12000000"
            app:layout_constraintEnd_toEndOf="@+id/guideline_1"/>

        <FrameLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#12000000"
            app:layout_constraintEnd_toEndOf="@+id/guideline_1"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            >
            <TextView
                android:id="@+id/t2_stir_1_rpm"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textSize ="14sp"
                android:layout_gravity="center"
                android:gravity="center"
                />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:id="@+id/t2_stir_2_rpm"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"


                android:textSize ="14sp"
                android:autoSizeStepGranularity="1px"
                android:layout_gravity="center"
                android:gravity="center"
                />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:id="@+id/t2_stir_3_rpm"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize ="14sp"
                android:autoSizeStepGranularity="1px"
                android:layout_gravity="center"
                android:gravity="center"
                />

        </LinearLayout>


        <FrameLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#12000000"
            app:layout_constraintEnd_toEndOf="@+id/guideline_1"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            >
            <TextView
                android:id="@+id/t2_stir_1_minute"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"


                android:textSize ="14sp"
                android:autoSizeStepGranularity="1px"
                android:layout_gravity="center"
                android:gravity="center"
                />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:id="@+id/t2_stir_2_minute"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize ="14sp"
                android:autoSizeStepGranularity="1px"
                android:layout_gravity="center"
                android:gravity="center"
                />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />
            <TextView
                android:id="@+id/t2_stir_3_minute"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"

                android:textSize ="14sp"
                android:autoSizeStepGranularity="1px"
                android:layout_gravity="center"
                android:gravity="center"
                />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#12000000"
                app:layout_constraintBottom_toBottomOf="parent" />

        </LinearLayout>




    </LinearLayout>



</LinearLayout>